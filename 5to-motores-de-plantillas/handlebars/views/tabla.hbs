<!-- historial table -->
<table class="table">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Precio</th>
            <th scope="col">Imagen</th>
        </tr>
    </thead>
    <tbody>
         <!--if no datos--> 
        {{#if (productos.length === 0)}} { 
            <tr>
                <td colspan="3">No hay datos</td>
            </tr>
        }{{#else}}{
        
        {{#for (let i = 0; i < productos.length; i++)}} { 
            <tr>
                <td>datos[i].nombre</td>
                <td>datos[i].apellido</td>
                <td>datos[i].edad</td>
            </tr>
         } }
    </tbody>